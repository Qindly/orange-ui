{
  "compilerOptions": {
    "strict": true,

    "types": ["react", "react-dom", "jest"],
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "orange-ui": ["src"],
      "orange-ui/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
